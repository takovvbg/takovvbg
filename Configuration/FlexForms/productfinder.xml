      <ROOT>
        <TCEforms>
          <sheetTitle>General</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>

          <settings.srcturl>
            <TCEforms>
              <label>Url</label>
              <config>
                <type>input</type>
                <size>200</size>                
              </config>
            </TCEforms>
          </settings.srcturl>  

          <settings.customerid>
            <TCEforms>
              <label>Customer ID</label>
              <config>
                <type>input</type>
                <size>50</size>                
              </config>
            </TCEforms>
          </settings.customerid>  

          <settings.mjsfilename>
            <TCEforms>
              <label>Mjs file name</label>
              <config>
                <type>input</type>
                <size>100</size>                
              </config>
            </TCEforms>
          </settings.mjsfilename>  

        </el>
      </ROOT>
    </sDEF>

  </sheets>
</T3DataStructure>