{
  "name": "eea/eea-components12",
  "type": "typo3-cms-extension",
  "version": "12.0.1",
  "description": "Adds Einfach E-Auto components to your application. Requires an active subscription.",
  "authors": [
        {
            "name": "Tashko Valkov",
            "role": "Developer"
        }
    ],
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "Eea\\EeaComponents12\\": "Classes"
    }
  },
  "extra": {
        "typo3/cms": {
            "extension-key": "eea_components12"
        }
  },
  "require-dev": {
      "typo3/testing-framework": "^6.9.0"
  },  
  "autoload-dev": {
      "psr-4": {
          "Eea\\EeaComponents12\\Tests\\": "Tests"
      }
  },
  "replace": {
      "typo3-ter/eea-components12": "self.version"
  },
  "config": {
      "vendor-dir": ".Build/vendor",
      "bin-dir": ".Build/bin"
  },
  "scripts": {
      "post-autoload-dump": [
          "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
      ]
  }
}
